<?xml version="1.0" encoding="windows-1251"?>
<root version="1.0">
    <questions>       
       <question id="1" name="1.Какой объект деятельности вас привлекает (сфера труда)?">
            <variants>
                 <variant id="1" text="1.Человек (дети и взрослые, ученики и студенты, клиенты и пациенты, покупатели и пассажиры, зрители, читатели, сотрудники). " character="1"/>
                 <variant id="2" text="2.Информация (тексты, формулы, схемы, коды, чертежи, иностранные языки, языки программирования)." character="4"/>
                 <variant id="3" text="3.Финансы (деньги, акции, фонды, лимиты, кредиты)." character="2"/>
                 <variant id="4" text="4.Техника (механизмы, станки, здания, конструкции, приборы, машины)." character="3"/>
                 <variant id="5" text="5.Искусство (литература, музыка, театр, кино, балет, живопись)." character="6"/>
                 <variant id="6" text="6.Животные (служебные, дикие, домашние, промысловые)." character="7"/>
                 <variant id="7" text="7.Растения (сельскохозяйственные, дикорастущие, декоративные)." character="8"/>
                 <variant id="8" text="8.Продукты питания (мясные, рыбные, молочные, кондитерские и хлебобулочные изделия, консервы, плоды, овощи, фрукты)." character="9"/>
                 <variant id="9" text="9.Изделия (металл, ткани, мех, кожа, дерево, камень, лекарства)." character="5"/>
                 <variant id="10" text="10.Природные ресурсы (земли, леса, горы, водоемы, месторождения)." character="10"/>
            </variants>
        </question>
       <question id="2" name="2.Какой вид деятельности вас привлекает?">
            <variants>
                 <variant id="1" text="1.Управление (руководство чьей-то деятельностью)." character="1"/>
                 <variant id="2" text="2.Облуживание (удовлетворение чьих-то потребностей)." character="2"/>
                 <variant id="3" text="3.Образование (воспитание, обучение, формирование личности)." character="3"/>
                 <variant id="4" text="4.Оздоровление (избавление от болезней и их предупреждение)." character="4"/>
                 <variant id="5" text="5.Творчество (создание оригинальных произведений искусства)." character="5"/>
                 <variant id="6" text="6.Производство (изготовление продукции)." character="6"/>
                 <variant id="7" text="7.Конструирование (проектирование деталей и объектов)." character="7"/>
                 <variant id="8" text="8.Исследование (научное изучение чего-либо или кого-либо)." character="8"/>
                 <variant id="9" text="9.Защита (охрана от враждебных действий)." character="9"/>
                 <variant id="10" text="10.Контроль (проверка и наблюдение)." character="10"/>
            </variants>
        </question>
    </questions>
</root>