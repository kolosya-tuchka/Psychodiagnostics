procedure TForm53.FormShow(Sender: TObject);
var
color,i,j,z:integer;
begin
Button2.Enabled:=False;
Button2.Visible:=False;
Button3.Enabled:=True;
Button4.Enabled:=False;
Button4.Visible:=False;
Button5.Enabled:=False;
Button5.Visible:=False;
plus_:=0;
minus_:=0;
cross_:=0;
memo1.clear;
  if (porka[6]='1') or (porka[6]='2') or (porka[6]='3') or (porka[6]='4') then
      begin
        symbol[6]:='-';
        symbol[7]:='-';
        symbol[8]:='-';
        minus_:=6;
        minus_kolvo:=3;
      end
      else
        begin
        symbol[7]:='-';
        symbol[8]:='-';
        minus_:=7;
        minus_kolvo:=2;
        end;
  if (porka[3]='6') or (porka[3]='7') or (porka[3]='0') then
      begin
        symbol[3]:='+';
        symbol[2]:='+';
        symbol[1]:='+';
        plus_:=3;
        plus_kolvo:=3;
      end
      else
      begin
        if (porka[2]='6') or (porka[2]='7') or (porka[2]='0') then
          begin
            symbol[2]:='+';
            symbol[1]:='+';
            plus_:=2;
            plus_kolvo:=2;
          end
          else
          begin
            if (porka[1]='6') or (porka[1]='7') or (porka[1]='0') then
              begin
              symbol[1]:='+';
              plus_:=1;
              plus_kolvo:=1;
              end
              else
              begin
              symbol[2]:='+';
              symbol[1]:='+';
              plus_:=2;
              plus_kolvo:=2;
              end;
          end;
      end;

    symbol[plus_+1]:='x';
    cross_:=plus_+1;
  if (porka[plus_+1]='1') or (porka[plus_+1]='2') or (porka[plus_+1]='3') or (porka[plus_+1]='4') then
    begin
    for i:=plus_+2 to minus_-1 do
        if (porka[i]='1') or (porka[i]='2') or (porka[i]='3') or (porka[i]='4') then
          begin
            symbol[i]:='x';
            cross_:=i;
            cross_kolvo:=cross_kolvo+1;
          end
         else
            break
     end
        else
          begin
            if (porka[plus_+1]='6') or (porka[plus_+1]='7') or (porka[plus_+1]='0') then
              begin
              for z:=plus_+2 to minus_-1 do
                if (porka[z]='6') or (porka[z]='7') or (porka[z]='0') then
                  begin
                    symbol[z]:='x';
                    cross_:=z;
                    cross_kolvo:=cross_kolvo+1;
                  end
                  else
                  break;
              end;
            end;
for j:=1 to 8 do
  begin
    if symbol[j]='' then
    begin
      symbol[j]:='=';
      equal_kolvo:=equal_kolvo+1;
    end;
  end;
    top:=0;
    Left:=0;
    begin
    height:=Screen.height-40;
    width:=Screen.width;
    end;
end;

procedure TForm53.Button2Click(Sender: TObject);
var
 Word: variant;
 Doc: variant;
 S: variant;
 i,j,x,y: integer;
begin
  try
    Word := CreateOleObject('Word.Application');
    Doc:=Word.Documents.Add;
  except
    ShowMessage('Не удается создать документ Microsoft Word');
  end;

S:=Word.Selection;
S.Font.Bold := 1;
S.Font.Size:=18;
S.ParagraphFormat.Alignment:=1;
S.TypeText('Тест Люшера');
S.TypeParagraph;
S.Font.Bold := 0;
S.Font.Size:=16;
S.ParagraphFormat.Alignment:=3;
S.Font.Bold := 1;
S.TypeText('Пользователь: ' );
S.Font.Bold := 0;
S.TypeText(Form2.label3.caption);
S.TypeParagraph;
S.Font.Bold := 1;
S.TypeText('Время прохождения: ');
S.Font.Bold := 0;
S.TypeText(Form53.label4.caption);
S.TypeParagraph;
S.Font.Bold := 1;
S.TypeText('Описание: ');
S.Font.Bold := 0;
S.TypeText(Form53.label5.caption);
S.TypeParagraph;
S.Font.Bold := 1;
S.TypeText('Результаты: ');
S.TypeParagraph;
S.Font.Bold := 0;
i:=0;
j:=0;
x:=0;
y:=0;
if PageControl1.ActivePageIndex=1 then
  begin
    if ((count_1 <> 0) and (count_2 <> 0)) then
      begin
        for i:=count_1 to count_2 do
          begin
            S.TypeText(' ' + Memo2.Lines[i]);
          end;
      end;
    if ((count_3 <> 0) and (count_4 <> 0)) then
      begin
        S.TypeParagraph;
        for j:=count_3 to count_4 do
          begin
            S.TypeText(' ' + Memo2.Lines[j]);
          end;
      end;
    if ((count_5 <> 0) and (count_6 <> 0)) then
      begin
        S.TypeParagraph;
        for x:=count_5 to count_6 do
          begin
            S.TypeText(' ' + Memo2.Lines[x]);
          end;
      end;
    if ((count_7 <> 0) and (count_8 <> 0)) then
      begin
        S.TypeParagraph;
        for y:=count_7 to count_8 do
          begin
            S.TypeText(' ' + Memo2.Lines[y]);
          end;
      end;
      if ((count_9 <> 0) and (count_10 <> 0)) then
      begin
        S.TypeParagraph;
        for i:=count_9 to count_10 do
          begin
            S.TypeText(' ' + Memo2.Lines[i]);
          end;
      end;
    if ((count_11 <> 0) and (count_12 <> 0)) then
      begin
        S.TypeParagraph;
        for j:=count_11 to count_12 do
          begin
            S.TypeText(' ' + Memo2.Lines[j]);
          end;
      end;
    if ((count_13 <> 0) and (count_14 <> 0)) then
      begin
        S.TypeParagraph;
        for x:=count_13 to count_14 do
          begin
            S.TypeText(' ' + Memo2.Lines[x]);
          end;
      end;
    if ((count_15 <> 0) and (count_16 <> 0)) then
      begin
        S.TypeParagraph;
        for y:=count_15 to count_16 do
          begin
            S.TypeText(' ' + Memo2.Lines[y]);
          end;
      end;
  Word.Visible := True;
  Word.Activate;
  Word.WindowState := 1;
  end
  else
    begin
      if ((count_1 <> 0) and (count_2 <> 0)) then
      begin
        S.TypeText(Memo1.Lines[count_1-1]);
        S.TypeParagraph;
        for i:=count_1 to count_2 do
          begin
            S.TypeText(' ' + Memo2.Lines[i]);
          end;
      end;
      if ((count_3 <> 0) and (count_4 <> 0)) then
      begin
        S.TypeParagraph;
        S.TypeText(Memo1.Lines[count_3-1]);
        S.TypeParagraph;
        for j:=count_3 to count_4 do
          begin
            S.TypeText(' ' + Memo2.Lines[j]);
          end;
      end;

    if ((count_5 <> 0) and (count_6 <> 0)) then
      begin
        S.TypeParagraph;
        S.TypeText(Memo1.Lines[count_5-1]);
        S.TypeParagraph;
        for x:=count_5 to count_6 do
          begin
            S.TypeText(' ' + Memo2.Lines[x]);
          end;
      end;
    if ((count_7 <> 0) and (count_8 <> 0)) then
      begin
        S.TypeParagraph;
        S.TypeText(Memo1.Lines[count_7-1]);
        S.TypeParagraph;
        for y:=count_7 to count_8 do
          begin
            S.TypeText(' ' + Memo2.Lines[y]);
          end;
      end;
      if ((count_9 <> 0) and (count_10 <> 0)) then
      begin
        S.TypeParagraph;
        S.TypeText(Memo1.Lines[count_9-1]);
        S.TypeParagraph;
        for i:=count_9 to count_10 do
          begin
            S.TypeText(' ' + Memo2.Lines[i]);
          end;
      end;
