  for i:=count_15+1 to count_16 do
  begin
  Memo2.Lines.Add(Memo1.Lines[i-1]);
  end;
    end;
  end;
   Button2.Visible:= True;
   Button2.Enabled:= True;
   Button3.Enabled:= False;
