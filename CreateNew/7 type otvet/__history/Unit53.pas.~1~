unit Unit53;

interface

uses
  WinApi.Windows, WinApi.Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, DB, Data.Win.ADODB, ExtCtrls, System.Win.ComObj, ComCtrls;

type
  TForm53 = class(TForm)
    SaveDialog1: TSaveDialog;
    DataSource1: TDataSource;
