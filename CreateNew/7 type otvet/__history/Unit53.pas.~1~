    TabSheet2: TTabSheet;
    Memo2: TMemo;
    Button4: TButton;
    Button5: TButton;
    Label1: TLabel;
    procedure FormShow(Sender: TObject);
    procedure Button2Click(Sender: TObject);
    procedure Button1Click(Sender: TObject);
    procedure Button3Click(Sender: TObject);
    procedure Button4Click(Sender: TObject);
    procedure Button5Click(Sender: TObject);
    procedure FormCreate(Sender: TObject);

   private
    function minus(minus_1,minus_value:integer):integer;
    function Plus__1(plus_1:integer):integer;
    function Plus(plus_1,plus_value:integer):integer;
    function Cross__1(cross_1:integer):integer;
    function Cross(cross_1,cross_value:integer):integer;
    function Equal__1(equal_1:integer):integer;
    function Equal(equal_1,equal_value:integer):integer;
     { Private declarations }
  public
    { Public declarations }
    porka:array[1..8]  of string;
    symbol:array[1..8] of string;
    minus_222:integer;
    Id:Integer;
    minus_kolvo,plus_kolvo,cross_kolvo,equal_kolvo:integer;
    minus_1,minus_2,minus_value,minus_s_value:integer;
    plus_1,plus_2,plus_value,plus_s_value:integer;
    cross_1,cross_value,cross_s_value:integer;
    equal_1,equal_value,equal_s_value:integer;
    minus_,plus_,cross_:integer;
    count_1,count_2,count_3,count_4:integer;
    count_5,count_6,count_7,count_8:integer;
    count_9,count_10,count_11,count_12:integer;
    count_13,count_14,count_15,count_16:integer;
    end;

var
  Form53: TForm53;

